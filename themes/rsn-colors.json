{
    "name": "RSN Semantic High Contrast",
    "tokenColors": [

        {
            "scope": "entity.name.type.rsn",
            "settings": {
                "foreground": "#C3E88D",
                "fontStyle": "bold"
            }
        },

        {
            "scope": "storage.type.key.primary.rsn",
            "settings": {
                "foreground": "#C792EA", 
                "fontStyle": "bold underline" 
            }
        },

        {
            "scope": ["storage.type.key.foreign.source.rsn", "storage.type.key.foreign.rsn"],
            "settings": {
                "foreground": "#89DDFF", 
                "fontStyle": "italic"
            }
        },
        {
            "scope": "storage.type.key.foreign.target.rsn",
            "settings": {
                "foreground": "#C792EA",
                "fontStyle": "bold"
            }
        },

        {
            "scope": "variable.key.candidate.rsn",
            "settings": {
                "foreground": "#FF66AA"
            }
        },

        {
            "scope": ["keyword.control.rsn", "keyword.operator.rsn"],
            "settings": {
                "foreground": "#82AAFF" 
            }
        },

        {
            "scope": ["constant.numeric.rsn", "constant.language.x.rsn"],
            "settings": {
                "foreground": "#F78C6C"
            }
        },

        {
            "scope": [
                "punctuation.definition.parameters.rsn",
                "punctuation.definition.begin.rsn",
                "punctuation.definition.end.rsn",
                "punctuation.separator.comma.rsn"
            ],
            "settings": {
                "foreground": "#8F93A2"
            }
        },
        {
            "scope": "text.plain.rsn",
            "settings": {
                "foreground": "#EEFFFF"
            }
        },
        {
            "scope": "punctuation.definition.key.hidden.rsn",
            "settings": {
                "foreground": "#00000000"
            }
        }
    ]
}