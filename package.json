{
    "name": "rsn-schema-highlighter",
    "displayName": "DBcoach — Formaler Syntax für Relationalen Entwurf",
    "version": "1.0",
    "publisher": "Karim Keshta",
    "repository": {
    "type": "git",
        "url": "https://github.com/KarimKeshta/rsn-schema-highlighter" 
    },
    "icon": "dbcoach-syntax-icon.png",
    "engines": {
        "vscode": "^1.80.0"
    },
    "contributes": {
        "languages": [
            {
                "id": "rsn",
                "aliases": ["Relational Schema Notation", "rsn"],
                "extensions": [".rsn"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "rsn",
                "scopeName": "source.rsn",
                "path": "./syntaxes/rsn.tmLanguage.json"
            }
        ],
        "themes": [
            {
                "label": "RSN Theme Overrides",
                "uiTheme": "vs-dark",
                "path": "./themes/rsn-colors.json"
            }
        ]
    }
}